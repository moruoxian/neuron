  "group_interval": 100,
  "endpoint_url": {
    "name": "Endpoint URL",
    "name_zh": "端点 URL",
    "description": "OPCUA server endpoint url",
    "description_zh": "OPCUA 服务器端点 URL",
    "attribute": "required",
    "type": "string",
    "default": "opc.tcp://127.0.0.1:4840/",
    "valid": {
      "regex": "/^opc\\.tcp:\\/\\/\\S+:\\d+(\\/[\\w\\-._~:\/?#\\[\\]@!$&'()*+,;=]*)?$/",
      "length": 256
    }
  },
  "username": {
    "name": "Username",

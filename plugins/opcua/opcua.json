    "attribute": "required",
    "type": "map",
    "default": 1,
    "valid": {
      "map": [
        {
          "key": "Enabled",
          "value": 1
        },
        {
          "key": "Disabled",
          "value": 0
        }
      ]
    }
  },
  "reconnect_interval": {
    "name": "Reconnect Interval (ms)",
